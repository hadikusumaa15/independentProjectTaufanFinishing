{{#each products }}
  <div class="col-sm-4 col-md-5">
    <div class="thumbnail">
      <img src="{{this.imagePath}}">
      <div class="caption">
        <h3>{{this.title}}</h3>
        <p>{{this.description}}</p>
        <div class="clearfix"> 
            <div class="price">Rp {{this.price}},-</div>
            <a href="#" class="btn btn-success pull-right" role="button">Buy Now!</a>
            
          </div>
            </div>
        </div>
    </div>
    {{/each}}